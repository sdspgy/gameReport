.boder {
  width: 90%;
  margin: 0 auto;
  /* border: 1px solid rgb(0, 255, 85); */
}

i-tabs {
  /* border: 1px solid rgb(0, 255, 85); */
  font-family: "Times New Roman", Georgia, Serif;
  font-size: 10px;
}

.tabBar {
  padding-top: 2px;
}

.tabBar .itabBar {
  float: right;
  height: 40px;
  width: 50%;
}

.containers {
  padding-top: 40px;
  /* border: 1px solid rgb(132, 0, 255); */
  width: 100%;
  height: 300px;
}

ec-canvas {
  /* border: 1px solid rgb(255, 0, 221); */
  width: 100%;
  height: 100%;
}

.warp {
  width: 100%;
  margin: 0 auto;
}

.table {
  border: 0px solid darkgray;
  /* position: relative;
  left: 60px; */
  z-index: -1;
  opacity: 0.9;
}

.tr {
  display: flex;
  width: 1100px;
  height: 2rem;
}

.td {
  width: 100px;
  /* display: flex; */
  height: 1.5rem;
  text-align: center;
  font-family: "Times New Roman", Georgia, Serif;
  font-size: 12px;
}

.bg-w {
  background: rgb(250, 254, 255);
}

.bg-g {
  background: #e6f3f9;
}

.th {
  width: 100px;
  color: rgb(83, 126, 155);
  /* display: flex; */
  height: 1.5rem;
  text-align: center;
  font-family: "Times New Roman", Georgia, Serif;
  font-size: 12px;
}

/* scroll-view滚动条隐藏 */

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
