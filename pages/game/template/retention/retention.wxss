.boder {
  width: 90%;
  margin: 0 auto;
  /* border: 1px solid rgb(0, 255, 85); */
}

i-tabs {
  /* border: 1px solid rgb(0, 255, 85); */
  font-family: "Times New Roman", Georgia, Serif;
  font-size: 10px;
}

.tabBar {
  position: relative;
  top: 5px;
}

/*.tabBar .itabBar {
  height: 50px;
  width: 50%;
} */

.containers {
  padding-top: 0px;
  /* border: 1px solid rgb(132, 0, 255); */
  width: 100%;
  height: 290px;
  position: relative;
  top: 5px;
}

ec-canvas {
  /* border: 1px solid rgb(255, 0, 221); */
  width: 100%;
  height: 100%;
}

.warp {
  width: 100%;
  padding-top: -20px;
}

view.collapse-item-index--i-collapse-item {
  border-top: none;
}

.table {
  border: 0px solid darkgray;
  /* position: relative;
  left: 60px; */
  z-index: -1;
  opacity: 1;
}

.tr {
  display: flex;
  width: 100%;
  height: 1rem;
}

.td {
  width: 100px;
  /* display: flex; */
  height: 1.5rem;
  text-align: center;
  font-family: "Times New Roman", Georgia, Serif;
  font-size: 15px;
}

.bg-w {
  background: rgb(250, 254, 255);
}

.bg-g {
  background: #e6f3f9;
}

.th {
  width: 100px;
  color: rgb(83, 126, 155);
  /* display: flex; */
  height: 1.5rem;
  text-align: center;
  font-family: "Times New Roman", Georgia, Serif;
  font-size: 12px;
}

/* scroll-view滚动条隐藏 */

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.system {
  width: 100%;
  height: 48px;
  background-color: rgba(0, 0, 0, 0);
  z-index: 2;
}

.os {
  position: relative;
  top: 7px;
}

.picker {
  padding-top: 5px;
}
