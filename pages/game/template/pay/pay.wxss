.chart-pane {
  width: 100%;
  height: 530rpx;
}

.chart {
  width:100%;
  height: 500rpx;
}

.chart-title {
  width: 100%;
  display: flex;
  height: 20rpx;
  flex-direction: column;
  align-items: center;
}

.chart-title-text {
  color:#495060;
  font-size:30rpx;
}

.table{
  display: flex;
  flex-direction: column;
  align-content: center;
  border: 1px solid red;
}

.table-row {
  display: flex;
  flex-direction: row;
  align-content: center;
}

.table-cell {
  height: 50px;
  font-size: 14px;
  margin: 0 10px;
  background-clor: gray;
  text-align: center;
  line-height: 50px;
}