.chart-pane {
  width: 100%;
  height: 450rpx;
}

.chart {
  margin-top: 15px;
}

.chart-body {
  width: 100%;
  height: 450rpx;
  margin-bottom: 10px;
}

.chart-title {
  width: 100%;
  display: flex;
  height: 20rpx;
  flex-direction: column;
  align-items: center;
  color: #495060;
  font-size: 14px;
  font-weight: bold;
  text-align: center;
  line-height: 20px;
}

.pop {
  width: 100%;
  padding-bottom: 1px;
}

.space {
  width: 90%;
  margin: 0 auto;
}

.space {
  margin-top: 5px;
}

.table {
  height: 640rpx;
  margin-top: 60rpx;
}

.bottom {
  position: absolute;
  top: 135%;
  width: 100%;
  height: 200px;
  z-index: -1;
}

.dividLine {
  margin: 0 auto;
  margin-top: 15px;
  height: 1px;
  background-color: #00c09c;
  width: 90%;
}

.chart-pie {
  width: 100%;
  height: 320rpx;
  margin-bottom: 5px;
  display: flex;
  justify-content: space-around;
}

.pieChart {
  width: 45%;
  height: 300rpx;
}

.pie-chart {
  position: relative;
  top: 30px;
}